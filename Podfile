  pod 'IGListKit'
  pod 'NotificationBannerSwift'
  pod 'Presentr'
  pod 'ReachabilitySwift'
  pod 'SnapKit'
  pod 'Socket.IO-Client-Swift'
  pod 'SwiftLint'
  pod 'SwiftyJSON'
end
