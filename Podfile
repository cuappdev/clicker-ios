  pod 'FutureNova', :git => 'https://github.com/cuappdev/ios-networking.git'
  pod 'Google/SignIn'
  pod 'IGListKit'
  pod 'Presentr'
  pod 'SnapKit'
  pod 'Socket.IO-Client-Swift'
  pod 'SwiftLint'
  pod 'SwiftyJSON'
  pod 'Sourcery'
end
